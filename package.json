{
  "name": "super-assign",
  "version": "1.0.0",
  "description": "Vitaminized object assignation with customizable options and inferred types",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && scripts/build.sh",
    "dev": "tsc --w --sourceMap",
    "clean": "rimraf dist",
    "test": "npm run lint && npm run ut",
    "lint": "eslint",
    "ut": "jest",
    "ut-dev": "node --inspect node_modules/jest/bin/jest.js --watch --runInBand",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier",
      "pre-push": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danikaze/super-assign.git"
  },
  "private": true,
  "keywords": [
    "typescript",
    "Object.assign",
    "extend-object",
    "extend",
    "deep assign",
    "jQuery",
    "deep extend",
    "deep",
    "patch"
  ],
  "author": "danikaze <danikaze@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danikaze/super-assign/issues"
  },
  "homepage": "https://github.com/danikaze/super-assign#readme",
  "devDependencies": {
    "@alex_neo/jest-expect-message": "^1.0.5",
    "@ef-carbon/tspm": "^2.2.5",
    "@types/jest": "^29.2.3",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.4",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "nyc": "^15.1.0",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {}
}
